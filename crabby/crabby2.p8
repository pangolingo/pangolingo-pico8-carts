pico-8 cartridge // http://www.pico-8.com
version 8
__lua__
--global game timer
t = 0
friction = 0.3

crab = {
	x = 64-12,
	y = 64-15,
	frms = {4,7,10},
	currfrm = 4,
	velx = 0,
	vely = 0,
	accelx = 0,
	accely = 0,
	accelx_u = 0,
	accely_u = 0,
	movspd = 0.5,
	maxspd = 1,
	width = 24,
	height = 24,
	rushspd = 6,
	rushcooldown = 0
}

function magnitude(x,y)
	return sqrt(x * x + y * y)
end

function crab_addaccel(x,y)
	crab.accelx += x
	crab.accely += y
end

function crab_addaccel_u(x,y)
	crab.accelx_u += x
	crab.accely_u += y
end

function crab_limitspd()
	mag = magnitude(crab.velx, crab.vely)
	
	if mag < 0.15 then
		crab.velx = 0
		crab.vely = 0
	end
	
	if mag > crab.maxspd then
		--normalize
		if mag != 0 then
			crab.velx = crab.velx / mag * crab.maxspd
			crab.vely = crab.vely / mag * crab.maxspd
		end
	end
end

function crab_screenloop()
	if crab.x > 128 then
		crab.x = 0 - crab.width
	end
	if crab.x + crab.width < 0 then
		crab.x = 128
	end
	if crab.y > 128 then
		crab.y = 0 - crab.height
	end
	if crab.y + crab.height < 0 then
		crab.y = 128
	end
end

function updatecrab()
	--physics!
	mag = magnitude(crab.velx, crab.vely)
	if mag != 0 then
		fricx = crab.velx / mag * -1 * friction
		fricy = crab.vely / mag * -1 * friction
		crab_addaccel(fricx,fricy)
	end
	
	crab.velx += crab.accelx
	crab.vely += crab.accely
	
	crab_limitspd()
	
	crab.velx += crab.accelx_u
	crab.vely += crab.accely_u
	
	crab_screenloop()
	
	crab.x += crab.velx
	crab.y += crab.vely
	

	crab.accelx = 0
	crab.accely = 0
	crab.accelx_u = 0
	crab.accely_u = 0
	
	if crab.rushcooldown > 1 then
		crab.rushcooldown -= 1
	end

 -- set the right sprite frame
	if magnitude(crab.velx,crab.vely) > 0 then
		frame = t % 24
		if frame < 8 then
			crab.currfrm = crab.frms[1]
		elseif frame < 16 then
			crab.currfrm = crab.frms[2]
		else
			crab.currfrm = crab.frms[3]
		end
	end
end

function drawcrab()
	--change trans from black to brown
	palt(0, false)
	palt(4, true)
	spr(crab.currfrm, crab.x, crab.y, 3,3)
end


--============
--go!
--============

function _init()
	--play the sweet music
	music(0)
end

function _update()
 --update global timer
	t += 1
	--update crabby
	updatecrab()
	
	-- buttons make crab move!
	-- left
	if btn(0) then
		crab_addaccel(-crab.movspd,0)
	end
	-- right
	if btn(1) then 
		crab_addaccel(crab.movspd,0)
	end
	-- up
	if btn(2) then 
		crab_addaccel(0,-crab.movspd)
	end
	-- down
	if btn(3) then 
		crab_addaccel(0,crab.movspd)
	end
	
	--rush!
	if btn(4) then
		mag = magnitude(crab.velx,crab.vely)
		
		if mag != 0  and crab.rushcooldown < 1 then
			gox = crab.velx / mag * crab.rushspd
			goy = crab.vely / mag * crab.rushspd
			crab_addaccel_u(gox,goy)
			--todo: fix cooldown
			--crab.rushcooldown = 0
		end
	end
	
end

function _draw()
	--blank the screen
	cls()
	
	--change sprite colors for rainbow
	for col=7,15 do
		newcol = col + t
		while newcol > 15 do
			newcol = newcol - 15 + 6
		end
		--change the color palette
		pal(col, newcol, 0)
	end
	
	--draw rainbow pixels
	--colors to draw are 7 thro 15
	--loop thro vert/horiz pixels
	k = 7
	for i=0,128 do
		for j=0,128 do
			x = j
			y = i
			col = k
			pset(x, y, col)
			
			k += 1
			if k > 15 then k = 7 end
		end
		--the k stuff goes here
		-- to make a horiz rainbow
	end
	

	--set palette back to normal
	pal()
	--crabby!
	drawcrab()
	
	color(2)
	print('arrows to move, z to rush',1,1)
	--print(crab.velx ..'/'.. crab.vely)
	--print(magnitude(crab.velx,crab.vely), 0, 10)
end
