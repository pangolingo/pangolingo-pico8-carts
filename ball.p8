--–––vector
-- this vector stuff feels hacky
-- using setmetatable
local vec = {}

function makevec(x, y)
	local t = {
		x = x,
		y = y
	}
	setmetatable(t, vec)
	return t
end

function vec.__add(a, b)
	return makevec(
		a.x + b.x,
		a.y + b.y
	)
end

function vec.__sub(a, b)
	return makevec(
		a.x - b.x,
		a.y - b.y
	)
end

function vec.__mul(a, b)
	if type(a) == "number" then
		return makevec(
			b.x * a, 
			b.y * a
		)
	elseif type(b) == "number" then
		return makevec(
			a.x * b, 
			a.y * b
		)
	end
end

function vec.__div(a, b)
	if type(a) == "number" then
		return makevec(
			b.x / a, 
			b.y / a
		)
	elseif type(b) == "number" then
		return makevec(
			a.x / b, 
			a.y / b
		)
	end
end

function dotprod(a, b)
	return a.x * b.x + a.y * b.y
end

function magnitude(a)
	return sqrt(a.x * a.x + a.y * a.y)
end

function normalize(a)
	local mag = magnitude(a)
	if mag != 0 then return a / mag end
	return makevec(0,0)
end

function limit(v, lim)
	if magnitude(v) > lim then
		return normalize(v) * lim
	end
	return v
end

function vec2str(a)
	return "(" .. a.x .. ", " .. a.y .. ")"
end

--–––ball
ball = {}
ball.loc = makevec(25, 20)
ball.velocity = makevec(0, 0)
ball.accel = makevec(0, 0)
ball.color = 14
ball.rad = 2.5
ball.mass = 5

function ballapplyforce(force)
	force /= ball.mass
	ball.accel += force
end

function ballmove(usraccel)
	friction = normalize(ball.velocity) * -1 * 0.05
	ballapplyforce(friction)
	ballapplyforce(makevec(0.005,0))
	--ballapplyforce(normalize(ball.velocity))
	-- linear acceleration: gravity
	ballapplyforce(makevec(0,0.1))
	-- random acceleration
	--ballapplyforce(makevec(rnd(1) - 0.5, rnd(1) - 0.5))
	-- user acceleration	
	ballapplyforce(usraccel)
	--ballapplyforce(makevec(0,1))
	
	ball.velocity += ball.accel
	ball.velocity = limit(ball.velocity, 4)
	
	ballcheckedges()
	ball.loc += ball.velocity
	
	ball.accel *= 0
end

function ballcheckedges()
	if ball.loc.x > 128 - bord_width
			or ball.loc.x < bord_width then
		ball.velocity.x = -ball.velocity.x
		ball.accel.x = -ball.accel.x
		sfx(0)
		--ball.velocity = makevec(0,0)
	end
	if ball.loc.y > 128 - bord_width
			or ball.loc.y < bord_width then
		ball.velocity.y = -ball.velocity.y
		ball.accel.y = -ball.accel.y
		sfx(0)
	end
end

function balldraw()
--[[
	circfill(ball.x + ball.radius,
					 ball.y + ball.radius, 
					 ball.radius, 
					 ball.color)
	print(ball.x)
	]]
	spr(1, 
					ball.loc.x - ball.rad, 
					ball.loc.y - ball.rad)
end

--–––lines
myline = {}
myline.start = makevec(64, 64)
myline.endd = makevec(0, 0)

function linedraw()
	line(
		myline.start.x, 
		myline.start.x,
		myline.endd.x,
		myline.endd.y
	)
end

function linemove()
	-- myline.endd = ball.loc
	myline.endd = normalize(ball.loc - myline.start) * 20 + myline.start
end
