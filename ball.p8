function _init()
		ballapplyforce(makevec(0, -5))
end

function _update()
	local x = 0
	local y = 0
	-- left
	if btn(0) then x = -1 end
	-- right
	if btn(1) then x = 1 end
	-- up
	if btn(2) then y = -1 end
	-- down
	if btn(3) then y = 1 end

	ballmove(makevec(x, y))
	linemove()
end

function _draw()
	cls()
	map(0, 0, 0, 0, 16, 16)
	balldraw()
	linedraw()
	
